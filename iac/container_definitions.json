[
    {
      "cpu": 256,
      "image": "${IMAGE}",
      "memory": 512,
      "name": "api",
      "networkMode": "awsvpc",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000
        }
      ],
      "environment": [
        {
          "name": "AWESOME_ENV_VAR",
          "value": "/mariaquiteria/aws_s3_region"
        },
        {
          "name": "POSTGRES_USER",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_PASSWORD",
          "value": "mariaquiteriadb"
        },
        {
          "name": "POSTGRES_ENDPOINT",
          "value": "mariaquiteriadb.cxt0v6aml06a.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "POSTGRES_NAME",
          "value": "mariaquiteria_db"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "mentoria-log",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "myawesomeapp"
        }
      }
    }
]
